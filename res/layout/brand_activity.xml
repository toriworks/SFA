<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="#00000000"
     >
     
    <RelativeLayout
        android:id="@+id/layoutBrandList"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />
     
    <RelativeLayout
        android:layout_width="220px"
        android:layout_height="1178px"
        android:layout_marginTop="53px"
        android:layout_marginLeft="580px"
        >
         <ImageView 
            android:layout_width="220px"
        	android:layout_height="match_parent"
        	android:layout_marginTop="40px"
          android:background="@drawable/at_brand_list_item_bg"   
            
         />
        <ImageView 
	     	android:layout_width="220px"
	        android:layout_height="40px"
	    	android:background="@drawable/at_brand_titlebar02"   
     	/>
       
            <ListView
		      	android:id="@+id/BrandList"
		      	android:layout_width="220px"
		        android:layout_height="1134px"
		        android:fadingEdgeLength="0px"
		        android:dividerHeight="0px"
		        android:divider="#00000000"
		        android:cacheColorHint="#00000000"
		        android:layout_marginTop="42px"
		      />
            
        </RelativeLayout>
</RelativeLayout>
