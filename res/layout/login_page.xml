<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:mySwitch="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="center"
     android:background="@drawable/at_bg01"
     >
     
    <RelativeLayout 
    	android:layout_width="518px"
    	android:layout_height="692px"    
        android:background="@drawable/at_loginbg"
     >
     <View 
         android:id="@+id/viewFA_Notice"
         android:layout_width="400px"
        	 android:layout_height="73px"
	         android:layout_marginTop="210px"
	         android:layout_marginLeft="59px"
	         android:visibility="gone"
      	android:background="@drawable/at_login_devicetext"   
      />
     
        <TableRow 
            android:id="@+id/rowLoginType"
             android:layout_width="400px"
        	 android:layout_height="73px"
	         android:layout_marginTop="210px"
	         android:layout_marginLeft="59px"
            android:gravity="center"
            >
     <ImageView 
         android:id="@+id/imgCommandCheck"
         android:layout_width="26px"
         android:layout_height="26px"
         android:background="@drawable/at_logincheck_o"
         
      />
     <TextView 
         android:id="@+id/txtHeadArea"
         android:layout_width="60px"
         android:layout_height="40px"
         android:text="본사"
         android:textColor="#ffffff"
         android:textSize="23px"
         android:gravity="center"
      />
     
     <RelativeLayout
         	android:layout_width="136px"
            android:layout_height="48px"
         >
     
     <fnc.salesforce.android.LIB.SlideButton
                    android:layout_width="136px"
                    android:layout_height="48px"
                    android:content="@+id/slideContentLayout"
                    android:handle="@+id/slideHandle"
                    android:background="@drawable/at_loginslide01"
                    android:orientation="horizontal"
                    android:id="@+id/slideButton">
                    <LinearLayout
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:background="#00000000"
                        android:orientation="horizontal"
                        android:gravity="bottom"
                        android:id="@+id/slideContentLayout">
                    </LinearLayout>
                    <Button
                        android:layout_width="48px"
                        android:layout_height="48px"
                        android:id="@+id/slideHandle"
                        android:background="@drawable/at_loginslide02"
                        />
                </fnc.salesforce.android.LIB.SlideButton>
                
    <LinearLayout
       android:layout_width="85px"
       android:layout_height="48px"
       android:layout_marginLeft="10px"
       android:orientation="horizontal"
       android:id="@+id/slideContentLayout1111"
       />
      </RelativeLayout>
     <TextView 
         android:id="@+id/txtShopArea"
         android:layout_width="60px"
         android:layout_height="40px"
         android:text="매장"
         android:textColor="#ffffff"
         android:textSize="23px"
         android:gravity="center"
      />
      <ImageView 
         android:id="@+id/imgShopCheck"
         android:layout_width="26px"
         android:layout_height="26px"
         android:background="@drawable/at_logincheck_n"
         
      />
        
     </TableRow>
     <View 
          android:layout_width="400px"
         android:layout_height="73px"
         android:layout_marginTop="290px"
         android:layout_marginLeft="59px"
      	android:background="@drawable/at_logintextbox01"   
      />
         <View 
          android:layout_width="28px"
         android:layout_height="28px"
         android:layout_marginTop="310px"
         android:layout_marginLeft="420px"
      	android:background="@drawable/at_login_icn_id"   
      />
         
         <EditText 
             android:id="@+id/editLoginID"
              android:layout_width="390px"
	         android:layout_height="73px"
	         android:layout_marginTop="290px"
	         android:layout_marginLeft="69px"
	         android:background="#00000000"
	         android:textColor="#ffffff"
	         android:hint="ID"
	         android:textSize="23px"
	         android:singleLine="true"
          />
         <EditText 
             android:id="@+id/editFA_Name"
              android:layout_width="390px"
	         android:layout_height="73px"
	         android:layout_marginTop="290px"
	         android:layout_marginLeft="69px"
	         android:background="#00000000"
	         android:textColor="#ffffff"
	         android:hint="NAME"
	         android:visibility="gone"
	         android:textSize="23px"
	         android:singleLine="true"
          />
     <View 
          android:layout_width="400px"
         android:layout_height="73px"
         android:layout_marginTop="375px"
         android:layout_marginLeft="59px"
      	android:background="@drawable/at_logintextbox01"   
      />
        <View 
          android:layout_width="28px"
         android:layout_height="28px"
         android:layout_marginTop="395px"
         android:layout_marginLeft="420px"
      	android:background="@drawable/at_login_icn_pw"   
      />
        <EditText 
            android:id="@+id/editLoginPass"
              android:layout_width="390px"
	         android:layout_height="73px"
	         android:layout_marginTop="375px"
	         android:layout_marginLeft="69px"
	         android:background="#00000000"
	         android:textColor="#ffffff"
	         android:hint="Password"
	         android:textSize="23px"
	         android:singleLine="true"
	         android:inputType="textPassword"
          />
        
        <EditText 
            android:id="@+id/editFA_Phone"
              android:layout_width="390px"
	         android:layout_height="73px"
	         android:layout_marginTop="375px"
	         android:layout_marginLeft="69px"
	         android:background="#00000000"
	         android:textColor="#ffffff"
	         android:hint="Phone"
	         android:textSize="23px"
	         android:singleLine="true"
	         android:visibility="gone"
	         android:inputType="text|number"
          />
        
        <CheckBox 
            android:id="@+id/check_ID_Save"
             android:layout_width="35px"
	         android:layout_height="35px"
	         android:layout_marginTop="452px"
	         android:layout_marginLeft="55px"
             android:visibility="invisible"
             android:checked="true"
         />
        
        <TextView 
            android:layout_width="135px"
	         android:layout_height="28px"
	         android:layout_marginTop="457px"
	         android:layout_marginLeft="105px"
	         android:text=" 아이디 저장"
            android:textColor="#aaffffff"
            android:gravity="center_vertical"
            android:textSize="18px"
            android:visibility="invisible"
            />
        <Button 
            android:id="@+id/btnLogin"
            android:layout_width="402px"
	         android:layout_height="76px"
	         android:layout_marginTop="520px"
	         android:layout_marginLeft="58px"
	         android:text="로그인"
	         android:textColor="#ffffff"
	         android:textSize="25px"
	         
	         android:background="@drawable/at_loginbtn01"   
         />
        
        <TableRow 
            android:id="@+id/rowUserChange"
            android:layout_width="402px"
	         android:layout_height="76px"
	         android:layout_marginTop="520px"
	         android:layout_marginLeft="58px"
	         android:gravity="center"
	          android:visibility="gone"
          >
            <Button 
            android:id="@+id/btnCancle"
            android:layout_width="132px"
	         android:layout_height="74px"
	         android:background="@drawable/at_sf_login_btncancel"   
         />
            <Button 
            android:id="@+id/btnLogOut"
            android:layout_width="132px"
	         android:layout_height="74px"
	         android:layout_marginLeft="2px"
	         android:background="@drawable/at_sf_login_btnlogout"   
         />
            <Button 
            android:id="@+id/btnUserChange"
           android:layout_width="132px"
	         android:layout_height="74px"
	         android:layout_marginLeft="2px"
	         android:background="@drawable/at_sf_login_btnmodify"   
         />
            
            
        </TableRow>
        
        <Button 
            android:id="@+id/btnFA_Input"
            android:layout_width="402px"
	         android:layout_height="76px"
	         android:layout_marginTop="520px"
	         android:layout_marginLeft="58px"
	         android:text="등록"
	         android:textColor="#ffffff"
	         android:textSize="25px"
	         android:visibility="gone"
	         android:background="@drawable/at_loginbtn01"   
         />
    </RelativeLayout>
</RelativeLayout>
