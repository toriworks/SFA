<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/at_bg01"
     >
     
    <ImageView 
        android:layout_width="800px"
    	android:layout_height="56px"
    	android:background="@drawable/at_titlebarbg"
    />
     <TextView 
        android:id="@+id/txt_AdGallery_Logo"
        android:layout_width="320px"
    	android:layout_height="40px"
    	android:layout_marginTop="8px"
    	android:layout_marginLeft="78px"
    	android:textColor="#ffffff"
    	android:textSize="20px"
    	android:singleLine="true"
    	android:gravity="center_vertical"
    />

    <ImageView 
        android:id="@+id/btnMenu_Activity"
        android:layout_width="40px"
    	android:layout_height="40px"
    	android:layout_marginTop="10px"
    	android:layout_marginLeft="10px"
    	android:visibility="gone"
    	android:background="@drawable/at_btn_menu"
    />
     <ImageView 
        android:id="@+id/btnGo_Main"
        android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="632px"
    	android:visibility="gone"
    	android:background="@drawable/im_sub_btn_home_n"
    />
   <ImageButton 
        android:id="@+id/btn_Brand_Activity"
        android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="692px"
    	android:visibility="gone"
    	android:background="@drawable/im_sub_btn_brand_n"
    />
   
   <ImageView 
        android:id="@+id/btnBack_Page"
       android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="752px"
    	android:background="@drawable/pdf_btn_close_n"
    />
    <!-- 
    	android:layout_marginLeft="697px"
    	 -->
    <ImageView 
        android:id="@+id/btn_Brand_LogOut"
        android:layout_width="40px"
    	android:layout_height="40px"
    	android:layout_marginTop="8px"
    	android:layout_marginLeft="752px"
    	android:visibility="gone"
    	android:background="@drawable/at_sub_btn_logout_n"
    />
    
    <Button 
        android:id="@+id/btn_Upload"
        android:layout_width="100px"
    	android:layout_height="40px"
    	android:layout_marginTop="7px"
    	android:layout_marginLeft="22px"
        android:text="가입 신청"
        android:textColor="#ffffff"
        android:background="@drawable/at_popup_btn_bg02"
    />
    
    <FrameLayout 
        android:id="@+id/layoutE_Form"
        android:layout_width="match_parent"
    	android:layout_height="match_parent"
    	android:layout_marginTop="60px"
        
   />

</RelativeLayout>
