     <ImageButton
        android:id="@+id/btnUserChange"
        android:layout_width="145px"
    	android:layout_height="72px"
    	android:layout_marginTop="1084px"
    	android:layout_marginLeft="1px"
    	android:background="@drawable/at_sf_main_btnmodify"
         
      />
    <RelativeLayout 
        android:id="@+id/layout_ScreenSaver"
        android:layout_width="145px"
    	android:layout_height="72px"
    	android:layout_marginTop="1157px"
    	android:layout_marginLeft="1px"
    	android:background="@drawable/at_main_menusv"
      />
     <HorizontalScrollView 
         android:id="@+id/horaThumb"
         android:layout_width="match_parent"
