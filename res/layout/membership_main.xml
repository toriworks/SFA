<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/at_bg01"
     >
     
    <ImageView 
        android:layout_width="800px"
    	android:layout_height="56px"
    	android:background="@drawable/at_titlebarbg"
    />
     <TextView 
        android:id="@+id/txt_AdGallery_Logo"
        android:layout_width="320px"
    	android:layout_height="40px"
    	android:layout_marginTop="8px"
    	android:layout_marginLeft="78px"
    	android:textColor="#ffffff"
    	android:textSize="20px"
    	android:singleLine="true"
    	android:gravity="center_vertical"
    />

    <ImageView 
        android:id="@+id/btnMenu_Activity"
        android:layout_width="40px"
    	android:layout_height="40px"
    	android:layout_marginTop="10px"
    	android:layout_marginLeft="10px"
    	android:background="@drawable/at_btn_menu"
    />
     <ImageView 
        android:id="@+id/btnGo_Main"
        android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="632px"
    	android:background="@drawable/im_sub_btn_home_n"
    />
   <ImageButton 
        android:id="@+id/btn_Brand_Activity"
        android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="692px"
    	android:background="@drawable/im_sub_btn_brand_n"
    />
   
   <ImageView 
        android:id="@+id/btnBack_Page"
       android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="752px"
    	android:background="@drawable/im_sub_btn_prev_n"
    />
    <!-- 
    	android:layout_marginLeft="697px"
    	 -->
    <ImageView 
        android:id="@+id/btn_Brand_LogOut"
        android:layout_width="40px"
    	android:layout_height="40px"
    	android:layout_marginTop="8px"
    	android:layout_marginLeft="752px"
    	android:visibility="gone"
    	android:background="@drawable/at_sub_btn_logout_n"
    />
    
    <TextView 
        android:id="@+id/txtCategoryName"
        android:layout_width="341px"
    	android:layout_height="42px"
    	android:layout_marginTop="71px"
    	android:layout_marginLeft="10px"
    	android:textColor="#ffffff"
    	android:textSize="32px"
    	android:gravity="center_vertical"
    />
    
    <ImageView 
        android:layout_width="26px"
    	android:layout_height="5px"
    	android:layout_marginTop="122px"
    	android:layout_marginLeft="10px"
    	android:background="@drawable/at_img_titleunderbar"
    />
    
   <LinearLayout 
        android:layout_width="328px"
    	android:layout_height="30px"
    	android:layout_marginTop="81px"
    	android:layout_marginLeft="462px"
    	android:gravity="right|center_vertical"
    	android:orientation="horizontal"
    	android:background="@drawable/at_sf_mem_joinbtn_bg"
 	>
 	
       <ImageView 
        android:id="@+id/btnE_Kolon_Card_Insert"
        android:layout_width="118px"
    	android:layout_height="28px"
    	android:background="@drawable/at_sf_mem_joinbtn_ekolon"
    />
       <ImageView 
           android:id="@+id/btn_Mobile_Insert"
        android:layout_width="118px"
    	android:layout_height="28px"
    	android:background="@drawable/at_sf_mem_joinbtn_momember"
    />
       </LinearLayout>
       
   <LinearLayout 
        android:layout_width="780px"
    	android:layout_height="40px"
    	android:layout_marginTop="140px"
    	android:layout_marginLeft="10px"
    	android:gravity="center"
    	android:orientation="horizontal"
 	>
 	
       <ImageView 
        android:id="@+id/btn_E_Kolon_Card_Intorduce"
        android:layout_width="248px"
    	android:layout_height="40px"
	    />
	      <ImageView 
	          android:id="@+id/btn_Brand_Grade_Intorduce"
		        android:layout_width="248px"
		    	android:layout_height="40px"
		    	android:layout_marginLeft="15px"
	    />
	      <ImageView 
	          android:id="@+id/btn_Customer_Search"
       android:layout_width="248px"
    	android:layout_height="40px"
    	android:layout_marginLeft="15px"
	    />
       </LinearLayout>
         
  <RelativeLayout 
		android:id="@+id/flipper"
        android:layout_width="780px"
    	android:layout_height="1010px"
    	android:layout_marginTop="190px"
    	android:layout_marginLeft="10px"
    	android:background="@drawable/at_sf_mem_cat_bg_01"
	/>
</RelativeLayout>
