<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="624px"
    android:layout_height="33px"
    android:gravity="center_vertical"
    android:background="#000000"
     >
    
    <ImageView 
        android:id="@+id/imgBack"
       	android:layout_width="624px"
    	android:layout_height="33px"
    	android:background="@drawable/at_lp_gridodd"
       />
     <TextView
         	android:id="@+id/txtSize"
    		android:layout_width="153px"
    		android:layout_height="33px"
    		android:textSize="14px"
    		android:layout_marginLeft="2px"
    		android:gravity="center"
    		android:textColor="#ffffff"
         />
        <TextView
            android:id="@+id/txtShopJaeGo"
			android:layout_width="150px"
    		android:layout_height="33px"	    
        	android:layout_marginLeft="157px"
    		android:textSize="14px"
    		android:gravity="right|center_vertical"
    		android:textColor="#89bd50"
         />
        
        <TextView
            android:id="@+id/txtAllJaeGo"
			android:layout_width="150px"
    		android:layout_height="33px"	    
        	android:layout_marginLeft="312px"
    		android:textSize="14px"
    		android:gravity="right|center_vertical"
    		android:textColor="#ffffff"
         />
        
        <TextView
            android:id="@+id/txtWhQtyJaeGo"
			android:layout_width="150px"
    		android:layout_height="33px"	    
        	android:layout_marginLeft="467px"
    		android:textSize="14px"
    		android:gravity="right|center_vertical"
    		android:textColor="#ffffff"
         />
        
</RelativeLayout>
