<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/at_bg01"
     >
     
    <ImageView 
        android:layout_width="800px"
    	android:layout_height="56px"
    	android:background="@drawable/at_titlebarbg"
    />
     <TextView 
        android:id="@+id/txt_AdGallery_Logo"
        android:layout_width="320px"
    	android:layout_height="40px"
    	android:layout_marginTop="8px"
    	android:layout_marginLeft="78px"
    	android:textColor="#ffffff"
    	android:textSize="20px"
    	android:singleLine="true"
    	android:gravity="center_vertical"
    />

    <ImageView 
        android:id="@+id/btnMenu_Activity"
        android:layout_width="40px"
    	android:layout_height="40px"
    	android:layout_marginTop="10px"
    	android:layout_marginLeft="10px"
    	android:background="@drawable/at_btn_menu"
    />
     <ImageView 
        android:id="@+id/btnGo_Main"
        android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="632px"
    	android:background="@drawable/im_sub_btn_home_n"
    />
   <ImageButton 
        android:id="@+id/btn_Brand_Activity"
        android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="692px"
    	android:background="@drawable/im_sub_btn_brand_n"
    />
   
   <ImageView 
        android:id="@+id/btnBack_Page"
       android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="752px"
    	android:background="@drawable/im_sub_btn_prev_n"
    />
    <!-- 
    	android:layout_marginLeft="697px"
    	 -->
    <ImageView 
        android:id="@+id/btn_Brand_LogOut"
        android:layout_width="40px"
    	android:layout_height="40px"
    	android:layout_marginTop="8px"
    	android:layout_marginLeft="752px"
    	android:visibility="gone"
    	android:background="@drawable/at_sub_btn_logout_n"
    />
    
    <TextView 
        android:id="@+id/txtCategoryName"
        android:layout_width="341px"
    	android:layout_height="42px"
    	android:layout_marginTop="91px"
    	android:layout_marginLeft="20px"
    	android:textColor="#ffffff"
    	android:textSize="32px"
    	android:gravity="center_vertical"
    />
    
    <ImageView 
        android:layout_width="26px"
    	android:layout_height="5px"
    	android:layout_marginTop="142px"
    	android:layout_marginLeft="20px"
    	android:background="@drawable/at_img_titleunderbar"
    />
    
   <LinearLayout 
        android:id="@+id/layoutCategoryView"
        android:layout_width="fill_parent"
    	android:layout_height="32px"
    	android:layout_marginTop="101px"
    	android:layout_marginLeft="371px"
    	android:layout_marginRight="20px"
    	android:gravity="right"
    	android:orientation="horizontal"
 	/>
    
     <HorizontalScrollView
         android:id="@+id/subCategory"
         android:layout_width="760px"
    	android:layout_height="40px"
    	android:layout_marginTop="176px"
    	android:layout_marginLeft="10px"
    	android:scrollbars="none"
    	android:background="@drawable/at_sub_tabbg"
     >
         <LinearLayout 
             android:id="@+id/layout_Tabbar"
              android:layout_width="wrap_content"
    			android:layout_height="40px"
    			android:orientation="horizontal"
          />
     </HorizontalScrollView>
     
  <RelativeLayout 
		android:id="@+id/flipper"
        android:layout_width="fill_parent"
    	android:layout_height="fill_parent"
    	android:layout_marginTop="230px"
    	android:background="#00000000"
	/>
  
  <LinearLayout 
        android:id="@+id/navigationRow"
        android:layout_width="fill_parent"
    	android:layout_height="15px"
    	android:layout_marginTop="1100px"
    	android:gravity="center"
    	android:orientation="horizontal"
     />
</RelativeLayout>
