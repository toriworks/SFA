<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/at_bg01"
     >
     
    <ImageView 
        android:layout_width="800px"
    	android:layout_height="56px"
    	android:background="@drawable/at_titlebarbg"
    />
     <TextView 
        android:id="@+id/txt_AdGallery_Logo"
        android:layout_width="320px"
    	android:layout_height="40px"
    	android:layout_marginTop="8px"
    	android:layout_marginLeft="78px"
    	android:textColor="#ffffff"
    	android:textSize="20px"
    	android:singleLine="true"
    	android:gravity="center_vertical"
    />

    <ImageView 
        android:id="@+id/btnMenu_Activity"
        android:layout_width="40px"
    	android:layout_height="40px"
    	android:layout_marginTop="10px"
    	android:layout_marginLeft="10px"
    	android:background="@drawable/at_btn_menu"
    />
     <ImageView 
        android:id="@+id/btnGo_Main"
        android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="632px"
    	android:background="@drawable/im_sub_btn_home_n"
    />
   <ImageButton 
        android:id="@+id/btn_Brand_Activity"
        android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="692px"
    	android:background="@drawable/im_sub_btn_brand_n"
    />
   
   <ImageView 
        android:id="@+id/btnBack_Page"
       android:layout_width="30px"
    	android:layout_height="30px"
    	android:layout_marginTop="13px"
    	android:layout_marginLeft="752px"
    	android:background="@drawable/im_sub_btn_prev_n"
    />
    <!-- 
    	android:layout_marginLeft="697px"
    	 -->
    <ImageView 
        android:id="@+id/btn_Brand_LogOut"
        android:layout_width="40px"
    	android:layout_height="40px"
    	android:layout_marginTop="8px"
    	android:layout_marginLeft="752px"
    	android:visibility="gone"
    	android:background="@drawable/at_sub_btn_logout_n"
    />
    
    <TextView 
        android:id="@+id/txtCategoryName"
        android:layout_width="341px"
    	android:layout_height="42px"
    	android:layout_marginTop="71px"
    	android:layout_marginLeft="10px"
    	android:textColor="#ffffff"
    	android:textSize="32px"
    	android:gravity="center_vertical"
    />
    
    <ImageView 
        android:layout_width="26px"
    	android:layout_height="5px"
    	android:layout_marginTop="122px"
    	android:layout_marginLeft="10px"
    	android:background="@drawable/at_img_titleunderbar"
    />
	 <RelativeLayout
	    android:id="@+id/webTeduri"
    	android:layout_width="780px"
    	android:layout_height="1040px"
    	android:layout_marginTop="150px"
    	android:layout_marginLeft="10px"
    >
	<RelativeLayout 
		android:id="@+id/webSecond"
		android:layout_height="match_parent" 
		android:layout_width="match_parent"
		android:background="#00000000"
	/>
	<ProgressBar
		android:id="@+id/progress"
		style="?android:attr/progressBarStyleHorizontal"
		android:layout_height="5px" 
		android:layout_width="fill_parent"
		android:max="100"
		android:visibility="visible"
	/>
	
	<FrameLayout
		android:id="@+id/SecondPage"
    	android:layout_width="fill_parent"
    	android:layout_height="fill_parent"
    	android:visibility="gone"
    />
     </RelativeLayout>
     
     
</RelativeLayout>